{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExtData",
  "type": "object",
  "required": [
    "encrypted_output1",
    "encrypted_output2",
    "ext_amount",
    "fee",
    "recipient",
    "relayer"
  ],
  "properties": {
    "encrypted_output1": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      },
      "maxItems": 32,
      "minItems": 32
    },
    "encrypted_output2": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      },
      "maxItems": 32,
      "minItems": 32
    },
    "ext_amount": {
      "type": "string"
    },
    "fee": {
      "type": "string"
    },
    "recipient": {
      "type": "string"
    },
    "relayer": {
      "type": "string"
    }
  }
}
