{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "State",
  "type": "object",
  "required": [
    "governor",
    "proposal_nonce"
  ],
  "properties": {
    "governor": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    },
    "proposal_nonce": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  }
}
