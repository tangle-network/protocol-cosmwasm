{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigResponse",
  "type": "object",
  "required": [
    "deposit_size",
    "handler",
    "proposal_nonce",
    "tokenwrapper_addr"
  ],
  "properties": {
    "deposit_size": {
      "type": "string"
    },
    "handler": {
      "type": "string"
    },
    "proposal_nonce": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "tokenwrapper_addr": {
      "type": "string"
    }
  }
}
